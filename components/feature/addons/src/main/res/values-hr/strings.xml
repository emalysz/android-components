    <string name="mozac_feature_addons_install_addon_content_description">Instaliraj dodatak</string>
    <!-- Indicates how many users have rated an add-on. %1$s will be replaced with number of users -->
    <string name="mozac_feature_addons_user_rating_count">Korisnici: %1$s</string>
